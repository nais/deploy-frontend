{
  "name": "nais-deploy-frontend",
  "version": "42.0.0",
  "description": "Frontend for nais deploy",
  "scripts": {
    "build-watch": "parcel watch frontend/index.html",
    "start-watch": "NODE_ENV=development nodemon bin/www",
    "start-offline": "NODE_ENV=development nodemon bin/offline",
    "dev": "concurrently --kill-others \"yarn run start-watch\" \"yarn run build-watch\"",
    "offline": "concurrently --kill-others \"yarn run start-offline\" \"yarn run build-watch\"",
    "build": "parcel build frontend/index.html",
    "test": "jest",
    "start": "node  ./bin/www"
  },
  "dependencies": {
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "express-session": "^1.17.0",
    "helmet": "^3.21.3",
    "http": "^0.0.0",
    "morgan": "^1.9.1",
    "openid-client": "^3.14.0",
    "passport": "^0.4.1",
    "pino": "^5.17.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "classnames": "^2.2.6",
    "concurrently": "^5.1.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "less": "^3.11.1",
    "lodash.throttle": "^4.1.1",
    "morgan-body": "^2.4.8",
    "nav-frontend-core": "^4.0.11",
    "nav-frontend-js-utils": "^1.0.8",
    "nav-frontend-knapper": "^1.0.39",
    "nav-frontend-knapper-style": "^0.3.34",
    "nav-frontend-paneler": "^1.0.24",
    "nav-frontend-paneler-style": "^0.3.18",
    "nav-frontend-typografi": "^2.0.17",
    "nav-frontend-typografi-style": "^1.0.19",
    "nodemon": "^2.0.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "typescript": "^3.8.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "frontend/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "author": "@navikt/aura",
  "license": "MIT",
  "private": true
}
