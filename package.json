{
  "name": "nais-deploy-frontend",
  "version": "42.0.0",
  "description": "Frontend for nais deploy",
  "scripts": {
    "build-watch": "parcel watch frontend/index.html",
    "start-watch": "NODE_ENV=development nodemon bin/www",
    "start-offline": "NODE_ENV=development nodemon bin/offline",
    "dev": "concurrently --kill-others \"yarn run start-watch\" \"yarn run build-watch\"",
    "offline": "concurrently --kill-others \"yarn run start-offline\" \"yarn run build-watch\"",
    "build": "parcel build frontend/index.html",
    "test": "jest",
    "tdd": "jest --watch",
    "start": "node  ./bin/www"
  },
  "dependencies": {
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "express-session": "^1.17.0",
    "helmet": "^3.21.3",
    "http": "^0.0.0",
    "morgan": "^1.9.1",
    "morgan-body": "^2.4.8",
    "openid-client": "^3.14.0",
    "passport": "^0.4.1",
    "pino": "^5.17.0",
    "redux-devtools-extension": "^2.13.8",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25.1.4",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.9.30",
    "babel-jest": "^25.1.0",
    "classnames": "^2.2.5",
    "concurrently": "^5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-transform-stub": "^2.0.0",
    "less": "^3.11.1",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.24.0",
    "nav-frontend-alertstriper": "^3.0.13",
    "nav-frontend-alertstriper-style": "^2.0.11",
    "nav-frontend-core": "^4.0.11",
    "nav-frontend-ikoner-assets": "^2.0.1",
    "nav-frontend-js-utils": "^1.0.8",
    "nav-frontend-knapper": "^2.0.1",
    "nav-frontend-knapper-style": "^0.3.35",
    "nav-frontend-lenker": "^1.0.34",
    "nav-frontend-lenker-style": "^0.2.25",
    "nav-frontend-lukknapp": "^1.0.25",
    "nav-frontend-lukknapp-style": "^0.2.22",
    "nav-frontend-modal": "^1.0.33",
    "nav-frontend-modal-style": "^0.3.32",
    "nav-frontend-paneler": "^1.0.24",
    "nav-frontend-paneler-style": "^0.3.18",
    "nav-frontend-spinner": "^2.0.2",
    "nav-frontend-spinner-style": "^0.2.5",
    "nav-frontend-typografi": "^2.0.17",
    "nav-frontend-typografi-style": "^1.0.19",
    "nav-frontend-veileder": "^2.0.16",
    "nav-frontend-veileder-style": "^2.0.13",
    "nav-frontend-veilederpanel": "^0.1.22",
    "nav-frontend-veilederpanel-style": "^0.0.25",
    "nodemon": "^2.0.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.4.2 || ^16.13.0",
    "react-collapse": "^5.0.1",
    "react-dom": "^16.13.0",
    "react-modal": "^3.11.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.13.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2",
    "yarn": "^1.22.4"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-babel",
    "transform": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleNameMapper": {
      "^.+.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "\\-style$": "<rootDir>/server/test/style-mock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/server/test/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "frontend/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "author": "@navikt/aura",
  "license": "MIT",
  "private": true
}
