{
  "name": "nais-deploy-frontend",
  "version": "42.0.0",
  "description": "Frontend for nais deploy",
  "scripts": {
    "build-watch": "parcel watch frontend/index.html",
    "start-watch": "NODE_ENV=development nodemon bin/www",
    "start-offline": "NODE_ENV=development nodemon bin/offline",
    "dev": "concurrently --kill-others \"yarn run start-watch\" \"yarn run build-watch\"",
    "offline": "concurrently --kill-others \"yarn run start-offline\" \"yarn run build-watch\"",
    "build": "parcel build frontend/index.html",
    "test": "jest",
    "tdd": "jest --watch",
    "start": "node  ./bin/www",
    "prepare": "husky install"
  },
  "dependencies": {
    "@navikt/ds-icons": "^0.2.1",
    "amplitude-js": "^8.1.0",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "express-session": "^1.17.1",
    "helmet": "^4.5.0",
    "memorystore": "^1.6.6",
    "morgan": "^1.10.0",
    "morgan-body": "^2.6.5",
    "nav-frontend-tabell-style": "^1.1.1",
    "openid-client": "^4.6.0",
    "parcel-bundler": "^1.12.5",
    "passport": "^0.4.1",
    "pino": "^6.11.3",
    "react-bootstrap": "^1.5.2",
    "react-timeago": "^5.2.0",
    "redux-devtools-extension": "^2.13.9",
    "serve-favicon": "^2.5.0",
    "zod": "^1.11.13"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.22",
    "@types/moment": "^2.13.0",
    "@types/react": "^17.0.3",
    "babel-jest": "^26.6.3",
    "classnames": "^2.3.1",
    "concurrently": "^6.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-transform-stub": "^2.0.0",
    "less": "^4.1.1",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.29.1",
    "nav-frontend-alertstriper": "^3.1.1",
    "nav-frontend-alertstriper-style": "^2.1.1",
    "nav-frontend-core": "^5.1.1",
    "nav-frontend-ikoner-assets": "^2.0.10",
    "nav-frontend-js-utils": "^1.0.17",
    "nav-frontend-knapper": "^2.1.1",
    "nav-frontend-knapper-style": "^1.1.1",
    "nav-frontend-lenker": "^1.1.1",
    "nav-frontend-lenker-style": "^1.1.1",
    "nav-frontend-lukknapp": "^1.1.1",
    "nav-frontend-lukknapp-style": "^1.1.1",
    "nav-frontend-modal": "^1.1.2",
    "nav-frontend-modal-style": "^1.1.1",
    "nav-frontend-paneler": "^2.1.1",
    "nav-frontend-paneler-style": "^1.1.1",
    "nav-frontend-spinner": "^2.0.16",
    "nav-frontend-spinner-style": "^1.0.2",
    "nav-frontend-typografi": "^3.1.1",
    "nav-frontend-typografi-style": "^1.1.1",
    "nav-frontend-veileder": "^2.1.2",
    "nav-frontend-veileder-style": "^2.1.1",
    "nav-frontend-veilederpanel": "^1.1.2",
    "nav-frontend-veilederpanel-style": "^1.1.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "prop-types": "^15.5.10",
    "react": "^17.0.2",
    "react-collapse": "^5.1.0",
    "react-dom": "^17.0.2",
    "react-modal": "^3.13.1",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^17.0.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "redux-saga-test-plan": "^4.0.1",
    "rimraf": "^3.0.2",
    "sinon": "^10.0.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4",
    "yarn": "^1.22.10"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-babel",
    "transform": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleNameMapper": {
      "^.+.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "\\-style$": "<rootDir>/server/test/style-mock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/server/test/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "frontend/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "author": "@navikt/aura",
  "license": "MIT",
  "private": true,
  "resolutions": {
    "**/**/node-forge": "^0.10.0"
  }
}
